type User {
  id: ID! @unique
  email: String @unique
  name: String!
  posts: [Post!]!
}

type Post {
  id: ID! @unique
  title: String!
  published: Boolean! @default(value: "false")
  author: User
}

type Position {
  id: ID! @unique
  x: String!
  y: String!
  z: String!
}

type Click {
  id: ID! @unique
  target: String!
}